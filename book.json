{
  "title": "数据科学实用手册",
  "author": "陈刘军",
   "structure": {
        "summary": "SUMMARY.md"
    },
	"plugins": [
        "mathjax",
		"copy-code-button",
		"github",
		"github-buttons",
		"splitter",
		"-sharing",
		"back-to-top-button"
		
    ],
	"pluginsConfig": {
	    "theme-default": {
            "showLevel": true
        },
		"fontSettings": {
			"theme": "white",
			"family": "msyh",
			"size": 2
		},
		"github": {
            "url": "https://github.com/LjunChen/Cook_Book_of_Data_Science"
        },
		"github-buttons": {
            "repo": "LjunChen/Cook_Book_of_Data_Science",
            "types": [
                "star",
				"watch",
				"fork"
            ],
			"size": "small"
        }
		
	},
  "pdf": {
    "pageNumbers": true, 
    "fontFamily": "Arial",
    "fontSize": 14,
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
  

}
  
